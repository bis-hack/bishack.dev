{
  "name": "bishack",
  "profile": "default",
  "static": {
    "dir": "public"
  },
  "error_pages": {
    "disable": true
  },
  "regions": [
    "us-east-1"
  ],
  "stages": {
    "development": {
      "proxy": {
        "command": "gin --port $PORT"
      }
    },
    "staging": {
      "domain": "stage.bishack.dev",
      "zone": false
    },
    "production": {
      "domain": "bishack.dev",
      "zone": false
    }
  }
}
